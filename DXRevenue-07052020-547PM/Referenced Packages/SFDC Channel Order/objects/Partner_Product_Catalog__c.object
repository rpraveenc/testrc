        <fullName>Fixed_Price_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL(Pricing_Type__c ,&quot;FIXED&quot;),ISNULL(  Fixed_Price__c ))</errorConditionFormula>
