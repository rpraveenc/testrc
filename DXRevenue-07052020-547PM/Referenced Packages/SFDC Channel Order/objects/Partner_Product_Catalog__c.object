    </validationRules>
    <validationRules>
        <fullName>Fixed_Price_should_be_blank</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL(Pricing_Type__c ,&quot;PNR&quot;),not(ISNULL( Fixed_Price__c) ))</errorConditionFormula>
